    "react-router": "^3.0.0",
    "react-router-redux": "^4.0.7",
    "redux": "^3.6.0",
    "redux-thunk": "^2.1.0",
    "resolve-url-loader": "^1.6.0",
    "sass-loader": "^4.0.2",
