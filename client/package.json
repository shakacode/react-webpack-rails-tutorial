    "babel-preset-es2015": "^6.18.0",
    "babel-preset-react": "^6.16.0",
    "babel-preset-stage-0": "^6.16.0",
    "babel-runtime": "^6.18.0",
    "bootstrap-loader": "^1.3.0",
    "bootstrap-sass": "^3.3.7",
    "classnames": "^2.2.5",
    "css-loader": "^0.26.0",
    "es5-shim": "^4.5.9",
    "expose-loader": "^0.7.1",
    "extract-text-webpack-plugin": "^1.0.1",
    "file-loader": "^0.9.0",
    "immutable": "^3.8.1",
    "imports-loader": "^0.6.5",
    "jquery": "^3.1.1",
    "jquery-ujs": "^1.2.2",
    "loader-utils": "^0.2.16",
    "lodash": "^4.17.2",
    "marked": "^0.3.6",
