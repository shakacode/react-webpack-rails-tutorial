{
  "name": "react-webpack-rails-tutorial",
  "sources": [
    {
      "dir": "client/app/bundles/comments/rescript",
      "subdirs": true
    }
  ],
  "package-specs": [
    {
      "module": "esmodule",
      "in-source": true
    }
  ],
  "bsc-flags": ["-open JsonCombinators", "-open Belt"],
  "suffix": ".bs.js",
  "bs-dependencies": [
    "@rescript/react",
    "@rescript/core",
    "@glennsl/rescript-fetch",
    "@glennsl/rescript-json-combinators",
    "rescript-react-on-rails"
  ],
  "jsx": {
    "version": 4,
    "mode": "automatic"
  }
}
